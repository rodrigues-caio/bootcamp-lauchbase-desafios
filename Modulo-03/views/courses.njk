{% extends 'layout.njk' %} {% block head %}
<link rel="stylesheet" href="./styles/pagina-posts-iframe.css" />
<link
  href="https://fonts.googleapis.com/icon?family=Material+Icons"
  rel="stylesheet"
/>
<title>Conteúdos!</title>
{% endblock %} {% block content %}
<main class="container">
  <div
    class="card"
    id="construindo-um-app-com-mapas-usando-react-native-maps-e-mapbox"
  >
    <img
      src="https://blog.rocketseat.com.br/content/images/2020/10/teste.jpg"
      alt="Construindo App com Mapa usando React Native Maps e MapBox"
    />
    <div class="card-info">
      <h2>Construindo App com Mapa usando React Native Maps e MapBox</h2>
      <p>por <strong>Thiago Marinho</strong></p>
      <span>8 MIN DE LEITURA</span>
    </div>
  </div>

  <div
    class="card"
    id="dark-mode-com-css-mudando-a-aparencia-do-blog-de-maneira-simples-e-rapida"
  >
    <img
      src="https://blog.rocketseat.com.br/content/images/2020/10/dark-mode-com-css-mudando-a-aparencia-do-blog-de-maneira-simples-e-rapida.jpg"
      alt="Dark Mode com CSS — mudando a aparência do Blog de maneira simples e rápida"
    />
    <div class="card-info">
      <h2>
        Dark Mode com CSS — mudando a aparência do Blog de maneira simples
      </h2>
      <p>por <strong>Thiago Marinho</strong></p>
      <span>3 MIN DE LEITURA</span>
    </div>
  </div>

  <div class="card" id="upload-de-imagens-no-s3-da-aws-com-node-js">
    <img
      src="https://blog.rocketseat.com.br/content/images/2020/09/upload-de-imagens-no-s3-da-aws-com-node-js.png"
      alt="Upload de imagens no S3 da AWS com Node.js"
    />
    <div class="card-info">
      <h2>Upload de imagens no S3 da AWS com Node.js</h2>
      <p>por <strong>Thiago Marinho</strong></p>
      <span>10 MIN DE LEITURA</span>
    </div>
  </div>
</main>

<div class="modal-container">
  <div class="modal">
    <a id="close"><i class="material-icons">close</i></a>
    <a id="maximaze"><span class="material-icons"> aspect_ratio </span></a>
    <a id="minimaze"><span class="material-icons"> close_fullscreen </span></a>
    <iframe src="" frameborder="0"></iframe>
  </div>
</div>

<script src="./scripts/modal.js"></script>
{% endblock %}
